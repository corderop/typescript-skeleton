{
  "name": "typescript-skeleton",
  "version": "0.0.0",
  "description": "Skeleton for any typescript project",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npx tsc",
    "test": "ENV=test && jest",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "format": "prettier --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corderop/typescript-skeleton.git"
  },
  "keywords": [
    "typescript",
    "skeleton"
  ],
  "author": "corderop",
  "bugs": {
    "url": "https://github.com/corderop/typescript-skeleton/issues"
  },
  "homepage": "https://github.com/corderop/typescript-skeleton#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
